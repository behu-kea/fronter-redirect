{
  "manifest_version": 2,
  "name": "Correct Fronter Redirect",
  "version": "1.0",
  "description": "Correctly redirects on Fronter",
  "icons": {
    "48": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": ["https://kea-fronter.itslearning.com/*"],
      "js": ["fronter-redirect.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@example.com",
      "strict_min_version": "42.0"
    }
  },
  "permissions": ["tabs", "storage"]
}
